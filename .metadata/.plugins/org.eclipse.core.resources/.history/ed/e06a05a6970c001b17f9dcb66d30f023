package sheepDrawing;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Polygon p = new Polygon(10, 10, 1, 10, 10);
		
		p.addPoint(90, 30);
		p.addPoint(100, 40);
